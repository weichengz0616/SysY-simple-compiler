







	.text
	.globl add
add:
	li t0, -16
	add sp, sp, t0

	sw ra, 12(sp)
	sw a0, 0(sp)

	sw a1, 4(sp)

	lw t0, 0(sp)
	sw t0, 8(sp)

	lw t0, 4(sp)
	sw t0, 12(sp)

	lw t0, 8(sp)
	lw t1, 12(sp)
	add t0, t0, t1
	sw t0, 16(sp)

	lw a0, 16(sp)
	lw ra, 12(sp)
	li t0, 16
	add sp, sp, t0
	ret


	.text
	.globl main
main:
	li t0, -16
	add sp, sp, t0

	sw ra, 12(sp)
	li a0, 1
	li a1, 2
	call add
	sw a0, 0(sp)
	lw a0, 0(sp)
	lw ra, 12(sp)
	li t0, 16
	add sp, sp, t0
	ret


