decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()

fun @add(%a_1: i32, %b_1: i32): i32
{
%entry:
	@a_1= alloc i32
	store %a_1 , @a_1
	@b_1= alloc i32
	store %b_1 , @b_1
	%0 = load @a_1
	%1 = load @b_1
	%2 = add %0, %1
	ret %2
}

fun @main(): i32
{
%entry:
	%3 = call @add(1, 2)
	ret %3
}

